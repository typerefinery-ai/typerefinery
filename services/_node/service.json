{
  "id": "node",
  "name": "Node v24.2.0",
  "description": "Portable node version.",
  "enabled": true,
  "status": "30",
  "logoutput": "",
  "icon": "pi pi-globe",
  "servicetype": 50,
  "servicelocation": 10,
  "execconfig": {
    "debuglog": true,
    "serviceorder": 1,
    "setuparchive": {
      "win32": {
        "name": "node-v24.2.0-win-x64.zip",
        "output": "node"
      },
      "darwin": {
        "name": "node-v24.2.0-darwin-x64.tar.gz",
        "output": "node"
      },
      "linux": {
        "name": "node-v24.2.0-linux-x64.tar.gz",
        "output": "node"
      }
    },
    "executable": {
      "win32": "node\\node-v24.2.0-win-x64\\node.exe",
      "darwin": "node/node-v24.2.0-darwin-x64/bin/node",
      "linux": "node/node-v24.2.0-linux-x64/bin/node",
      "default": "node\\node-v24.2.0-win-x64\\node.exe"
    },
    "executablecli": {
      "win32": "node\\node-v24.2.0-darwin-x64\\node_modules\\npm\\bin\\npm-cli.js",
      "darwin": "node/node-v24.2.0-darwin-x64/lib/node_modules/npm/bin/npm-cli.js",
      "linux": "node/node-v24.2.0-linux-x64/lib/node_modules/npm/bin/npm-cli.js",
      "default": "node\\node-v24.2.0-win-x64\\node_modules\\npm\\bin\\npm-cli.js"
    },
    "globalenv": {
      "NODE": "${SERVICE_EXECUTABLE}",
      "NODE_HOME": "${SERVICE_EXECUTABLE_HOME}",
      "NPM": "${SERVICE_EXECUTABLE_CLI}"
    }
  }
}
